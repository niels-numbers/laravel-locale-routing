{
    "name": "niels-numbers/laravel-localization",
    "description": "Detects the userâ€™s preferred language and redirects to the matching localized URL.",
    "keywords": [
        "php",
        "laravel",
        "localization",
        "locale",
        "language",
        "country",
        "routes",
        "routing",
        "translation"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Dr. Adam Nielsen",
            "email": "info@adam-nielsen.de"
        }
    ],
    "require": {
      "php": "^8.1",
      "illuminate/support": "^9|^10|^11|^12"
    },
    "require-dev": {
        "mockery/mockery": "^1.3.3",
        "orchestra/testbench": "^8.0",
        "phpunit/phpunit": "^10.0"
    },
    "autoload": {
        "psr-4": {
            "NielsNumbers\\LocaleRouting\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "NielsNumbers\\LocaleRouting\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "NielsNumbers\\LocaleRouting\\ServiceProvider"
            ]
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
